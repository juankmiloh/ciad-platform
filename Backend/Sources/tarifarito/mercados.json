{
    "name": "mercados",
    "query": [
        "SELECT ",
        "    DISTINCT ID_MERCADO,",
        "    NOM_MERCADO,",
        "    ESTADO ",
        "FROM ",
        "    CARG_COMERCIAL_E.MERCADO_EMPRESA ",
        "WHERE ",
        "    ID_MERCADO = :MERCADO_ARG OR 0 = :MERCADO_ARG ",
        "    AND NOM_MERCADO NOT LIKE '%Mercado Prueba%' ",
        "    AND NOM_MERCADO NOT LIKE '%Mercado de Prueba%' ",
        "ORDER BY ",
        "    NOM_MERCADO"
    ]
}